{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build-client",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/Client/Client.csproj"
      ],
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "build-silohost",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/SiloHost/SiloHost.csproj"
      ],
      "problemMatcher": "$msCompile",
      "group": "build"
    }
  ]
}
